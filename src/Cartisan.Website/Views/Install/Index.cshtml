    <!-- END SIDEBAR TOGGLER BUTTON -->
    <!-- BEGIN LOGO -->
    <div class="logo">
        <a href="index.html">
            <img src="../../assets/admin/layout/img/logo-big.png" alt="" />
        </a>
    </div>
    <!-- END LOGO -->
    <!-- BEGIN LOGIN -->
    <div class="content">
        <!-- BEGIN REGISTRATION FORM -->
        <form class="register-form" action="index.html" method="post">
            <h3>安装</h3>
            <p class="hint">
                数据库信息
            </p>
            <div class="form-group">
                <label class="control-label visible-ie8 visible-ie9">数据库服务器类型</label>
                <select name="country" class="form-control">
                    <option value="SqlServer">SqlServer</option>
                    <option value="MySql">MySql</option>
                </select>
            </div>
            <div class="form-group">
                <label class="control-label visible-ie8 visible-ie9">数据库服务器</label>
                <input class="form-control placeholder-no-fix" type="text" placeholder="数据库服务器" name="fullname" />
            </div>
            <div class="form-group">
                <label class="control-label visible-ie8 visible-ie9">数据库名称</label>
                <input class="form-control placeholder-no-fix" type="text" placeholder="数据库名称" name="address" />
            </div>
            <div class="form-group">
                <label class="control-label visible-ie8 visible-ie9">数据库用户帐号</label>
                <input class="form-control placeholder-no-fix" type="text" placeholder="数据库用户帐号" name="city"/>
            </div>
            <div class="form-group">
                <label class="control-label visible-ie8 visible-ie9">数据库密码</label>
                <input class="form-control placeholder-no-fix" type="password" autocomplete="off" id="database_password" placeholder="数据库密码" name="password" />
            </div>
            <p class="hint">
                创始人信息:
            </p>
            <div class="form-group">
                <label class="control-label visible-ie8 visible-ie9">管理员账号</label>
                <input class="form-control placeholder-no-fix" type="text" autocomplete="off" placeholder="管理员账号" name="username" />
            </div>
            <div class="form-group">
                <label class="control-label visible-ie8 visible-ie9">密码</label>
                <input class="form-control placeholder-no-fix" type="password" autocomplete="off" id="register_password" placeholder="密码" name="password" />
            </div>
            <div class="form-group">
                <label class="control-label visible-ie8 visible-ie9">确认密码</label>
                <input class="form-control placeholder-no-fix" type="password" autocomplete="off" placeholder="确认密码" name="rpassword" />
            </div>
            <div class="form-group margin-top-20 margin-bottom-20">
                <label class="check">
                    <input type="checkbox" name="tnc" /> 导入测试库数据
                </label>
                <div id="register_tnc_error">
                </div>
            </div>
            <div class="form-actions">
                <button type="submit" id="register-submit-btn" class="btn btn-success uppercase pull-right">安装</button>
            </div>
        </form>
        <!-- END REGISTRATION FORM -->
    </div>
